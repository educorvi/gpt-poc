{
  "name": "@educorvi/gpt-frontend",
  "version": "0.1.2",
  "scripts": {
    "dev": "vite",
    "build": "yarn run build-app && yarn run build-webcomponent",
    "build-app": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-webcomponent": "vite -c webcomponent.vite.config.ts build",
    "type-check": "vue-tsc --noEmit",
    "prepare": "yarn run build-webcomponent"
  },
  "files": [
    "webcomponent_dist"
  ],
  "dependencies": {
    "bootstrap": "5",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "npm-run-all": "^4.1.5",
    "sass": "^1.62.1",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vue-tsc": "^1.2.0"
  },
  "packageManager": "yarn@3.5.0"
}
